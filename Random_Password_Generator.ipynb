    "import string\n",
    "from random import *\n",
    "\n",
    "characters = string.ascii_letters + string.punctuation  + string.digits\n",
    "password =  \"\".join(choice(characters) for x in range(randint(8, 16)))\n",
    "\n",
    "print(\"Random password: \" + password)"
